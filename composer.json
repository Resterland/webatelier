{
    "name": "resterland/webatelier",
    "description": "Source of any Resterland TYPO3 projects",
    "keywords": ["typo3", "extension", "distribution"],
    "homepage": "https://resterland.ch",
    "authors": [
        {
            "name": "Roland Fuhrer",
            "email": "roland@resterland.ch",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "require": {
        "helhum/typo3-console": "^7 || ^8",
        "helhum/typo3-secure-web": "^0.3",
        "typo3/cms-adminpanel": "^11 || ^12",
        "typo3/cms-belog": "^11 || ^12",
        "typo3/cms-beuser": "^11 || ^12",
        "typo3/cms-core": "^11 || ^12",
        "typo3/cms-dashboard": "^11 || ^12",
        "typo3/cms-felogin": "^11 || ^12",
        "typo3/cms-filemetadata": "^11 || ^12",
        "typo3/cms-fluid-styled-content": "^11 || ^12",
        "typo3/cms-form": "^11 || ^12",
        "typo3/cms-impexp": " ^11 || ^12",
        "typo3/cms-indexed-search": " ^11 || ^12",
        "typo3/cms-info": "^11 || ^12",
        "typo3/cms-linkvalidator": "^11 || ^12",
        "typo3/cms-lowlevel": "^11 || ^12",
        "typo3/cms-redirects": "^11 || ^12",
        "typo3/cms-reports": "^11 || ^12",
        "typo3/cms-rte-ckeditor": "^11 || ^12",
        "typo3/cms-scheduler": "^11 || ^12",
        "typo3/cms-seo": "^11 || ^12",
        "typo3/cms-setup": "^11 || ^12",
        "typo3/cms-sys-note": "^11 || ^12",
        "typo3/cms-t3editor": "^11 || ^12",
        "typo3/cms-tstemplate": "^11 || ^12",
        "typo3/cms-viewpage": "^11 || ^12"
    }
,
    "require-dev": {
        "typo3/testing-framework": "^6 || ^8"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": "public",
            "root-dir": "private",
            "extension-key": "webatelier"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "typo3/cms-*": true,
            "resterland/*": true,
            "roave/security-advisories": true,
            "typo3/testing-framework": true,
            "helhum/*": true,
            "typo3/class-alias-loader": true
        }
    }
}
