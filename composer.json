{
	"name": "resterland/webatelier",
	"description": "Source of any Resterland TYPO3 projects",
	"keywords": ["typo3", "extension", "distribution"],
	"homepage": "https://resterland.ch",
	"authors": [
		{
			"name": "Roland Fuhrer",
			"email": "roland@resterland.ch",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"require": {
		"helhum/typo3-console": "^6.3",
		"helhum/typo3-secure-web": "^0.2.10",
		"typo3/cms-core": "^10.4",
		"roave/security-advisories": "dev-master",
		"typo3/cms-adminpanel": "^10.4",
		"typo3/cms-belog": "^10.4",
		"typo3/cms-beuser": "^10.4",
		"typo3/cms-form": "^10.4",
		"typo3/cms-scheduler": "^10.4",
		"typo3/cms-impexp": "^10.4",
		"typo3/cms-redirects": "^10.4",
		"typo3/cms-reports": "^10.4",
		"typo3/cms-info": "^10.4",
		"typo3/cms-lowlevel": "^10.4",
		"typo3/cms-sys-note": "^10.4",
		"typo3/cms-setup": "^10.4",
		"typo3/cms-tstemplate": "^10.4",
		"typo3/cms-viewpage": "^10.4",
		"typo3/cms-linkvalidator": "^10.4",
		"typo3/cms-t3editor": "^10.4",
		"typo3/cms-rte-ckeditor": "^10.4",
		"typo3/cms-filemetadata": "^10.4",
		"typo3/cms-indexed-search": "^10.4",
		"typo3/cms-seo": "^10.4",
		"typo3/cms-fluid-styled-content": "^10.4"
	}
	,
	"require-dev": {
		"helhum/dotenv-connector": "^2.2",
		"typo3/testing-framework": "^5"
	},
	"replace": {
		"resterland/webatelier": "self.version",
		"typo3-ter/webatelier": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"web-dir": "public",
			"root-dir": "private"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true
	}
}
