{
    "name": "resterland/webatelier",
    "description": "Source of any Resterland TYPO3 projects",
    "keywords": ["typo3", "extension", "distribution"],
    "homepage": "https://resterland.ch",
    "authors": [
        {
            "name": "Roland Fuhrer",
            "email": "roland@resterland.ch",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "require": {
        "helhum/typo3-config-handling": "^1.0",
        "helhum/typo3-console": "^6",
        "helhum/typo3-secure-web": "^0.2",
        "typo3/cms-core": "^10",
        "typo3/cms-adminpanel": "^10",
        "typo3/cms-belog": "^10",
        "typo3/cms-beuser": "^10",
        "typo3/cms-form": "^10",
        "typo3/cms-scheduler": "^10",
        "typo3/cms-impexp": "^10",
        "typo3/cms-redirects": "^10",
        "typo3/cms-reports": "^10",
        "typo3/cms-info": "^10",
        "typo3/cms-lowlevel": "^10",
        "typo3/cms-sys-note": "^10",
        "typo3/cms-setup": "^10",
        "typo3/cms-tstemplate": "^10",
        "typo3/cms-viewpage": "^10",
        "typo3/cms-linkvalidator": "^10",
        "typo3/cms-t3editor": "^10",
        "typo3/cms-rte-ckeditor": "^10",
        "typo3/cms-filemetadata": "^10",
        "typo3/cms-indexed-search": "^10",
        "typo3/cms-seo": "^10",
        "typo3/cms-fluid-styled-content": "^10"
    }
,
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "helhum/dotenv-connector": "^2",
        "typo3/testing-framework": "^5 || ^6"
    },
    "replace": {
        "resterland/webatelier": "self.version",
        "typo3-ter/webatelier": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": "public",
            "root-dir": "private"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
