{
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		},
		{
			"type": "vcs",
			"url": "https://github.com/resterland/webatelier.git"
		}
	],
	"name": "resterland/webatelier",
	"description": "Source of any Resterland TYPO3 projects",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Roland Fuhrer",
			"email": "roland@resterland.ch",
			"homepage": "https://resterland.ch",
			"role": "Developer"
		}
	],
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "7.2"
		}
	},
	"require": {
		"helhum/typo3-console": "^5.7",
		"helhum/typo3-secure-web": "^0.2",
		"roave/security-advisories": "dev-master",
		"typo3/cms-adminpanel": "^9.5 || ^10.1",
		"typo3/cms-belog": "^9.5 || ^10.1",
		"typo3/cms-beuser": "^9.5 || ^10.1",
		"typo3/cms-core": "^9.5 || ^10.1",
		"typo3/cms-felogin": "^9.5 || ^10.1",
		"typo3/cms-filemetadata": "^9.5 || ^10.1",
		"typo3/cms-form": "^9.5 || ^10.1",
		"typo3/cms-impexp": "^9.5 || ^10.1",
		"typo3/cms-indexed-search": "^9.5 || ^10.1",
		"typo3/cms-info": "^9.5 || ^10.1",
		"typo3/cms-linkvalidator": "^9.5 || ^10.1",
		"typo3/cms-lowlevel": "^9.5 || ^10.1",
		"typo3/cms-redirects": "^9.5 || ^10.1",
		"typo3/cms-reports": "^9.5 || ^10.1",
		"typo3/cms-rte-ckeditor": "^9.5 || ^10.1",
		"typo3/cms-scheduler": "^9.5 || ^10.1",
		"typo3/cms-seo": "^9.5 || ^10.1",
		"typo3/cms-setup": "^9.5 || ^10.1",
		"typo3/cms-sys-note": "^9.5 || ^10.1",
		"typo3/cms-t3editor": "^9.5 || ^10.1",
		"typo3/cms-tstemplate": "^9.5 || ^10.1",
		"typo3/cms-viewpage": "^9.5 || ^10.1",
		"causal/image_autoresize": ">=1.9 <2.0.0"
	},
	"require-dev": {
		"helhum/dotenv-connector": "^2.0",
		"typo3-console/composer-typo3-auto-install": "^0.3",
		"friendsofphp/php-cs-fixer": "^2.0",
		"typo3/testing-framework": "^4.11"
	},
	"extra": {
		"typo3/cms": {
			"root-dir": "private",
			"web-dir": "public"
		}
	}
}
